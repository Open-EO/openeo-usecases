{
  "process_graph": {
    "load_collection": {
      "arguments": {
        "id": "SIG0",
        "spatial_extent": {
          "west": 16.06,
          "south": 48.06,
          "east": 16.65,
          "north": 48.35
        },
        "bands": [
          "VV",
          "VH"
        ],
        "temporal_extent": [
          "2017-03-01",
          "2017-03-03"
        ]
      },
      "process_id": "load_collection"
    },
    "t_mean": {
      "arguments": {
        "data": {
          "from_node": "load_collection"
        },
        "dimension": "t",
        "reducer": {
          "process_graph": {
            "mean": {
              "arguments": {
                "data": {
                  "from_parameter": "data"
                }
              },
              "process_id": "mean",
              "result": true
            }
          }
        }
      },
      "process_id": "reduce_dimension"
    },
    "cr": {
      "arguments": {
        "data": {
          "from_node": "t_mean"
        },
        "dimension": "bands",
        "reducer": {
          "process_graph": {
            "VV": {
              "arguments": {
                "data": {
                  "from_parameter": "data"
                },
                "label": "VV"
              },
              "process_id": "array_element"
            },
            "VH": {
              "arguments": {
                "data": {
                  "from_parameter": "data"
                },
                "label": "VH"
              },
              "process_id": "array_element"
            },
            "sub": {
              "arguments": {
                "x": {
                  "from_node": "VH"
                },
                "y": {
                  "from_node": "VV"
                }
              },
              "process_id": "subtract",
              "result": true
            }
          }
        }
      },
      "process_id": "reduce_dimension"
    },
    "add_dim_cr": {
      "arguments": {
        "data": {
          "from_node": "cr"
        },
        "name": "bands",
        "label": "B",
        "type": "bands"
      },
      "process_id": "add_dimension"
    },
    "VV": {
      "arguments": {
        "data": {
          "from_node": "t_mean"
        },
        "bands": [
          "VV"
        ]
      },
      "process_id": "filter_bands"
    },
    "rename_label_VV": {
      "arguments": {
        "data": {
          "from_node": "VV"
        },
        "dimension": "bands",
        "target": [
          "R"
        ]
      },
      "process_id": "rename_labels"
    },
    "VH": {
      "arguments": {
        "data": {
          "from_node": "t_mean"
        },
        "bands": [
          "VH"
        ]
      },
      "process_id": "filter_bands"
    },
    "rename_label_VH": {
      "arguments": {
        "data": {
          "from_node": "VH"
        },
        "dimension": "bands",
        "target": [
          "G"
        ]
      },
      "process_id": "rename_labels"
    },
    "RG": {
      "arguments": {
        "cube1": {
          "from_node": "rename_label_VV"
        },
        "cube2": {
          "from_node": "rename_label_VH"
        }
      },
      "process_id": "merge_cubes"
    },
    "RGB": {
      "arguments": {
        "cube1": {
          "from_node": "RG"
        },
        "cube2": {
          "from_node": "add_dim_cr"
        }
      },
      "process_id": "merge_cubes"
    },
    "save_result": {
      "arguments": {
        "data": {
          "from_node": "RGB"
        },
        "format": "GTiff",
        "options": {
          "red": "R",
          "green": "G",
          "blue": "B"
        }
      },
      "process_id": "save_result",
      "result": true
    }
  }
}